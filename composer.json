{
    "name": "lmande/security-forcer",
    "description": "A package to force laravel developers' for composer security",
    "type": "library",
    "license": "MIT",
    "autoload" : {
    	"psr-4": {
    		"Lmande\\SecurityForcer\\": "src/"
    	}
    },
    "require": {
        "php": "^7.2.5|^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lmande\\SecurityForcer\\SecurityForcerServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/framework": ">=7.0.0"
    }
}
